{
  "name": "question-bank",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/question-bank/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/question-bank/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/question-bank"],
      "options": {
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "mkdir -p apps/next-app/public/content",
          "cp -r libs/question-bank/content/media apps/next-app/public/content"
        ],
        "parallel": false
      }
    }
  },
  "tags": [],
  "implicitDependencies": []
}
